{
    "name": "New Tab",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "New Tab",
    "host_permissions": [
        "https://www.google.com/complete/search",
        "https://www.baidu.com/sugrec"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.bilibili.com/*"
            ],
            "js": [
                "bilibili.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "https://*.*.*/*"
            ],
            "js": [
                "jumpserver.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "https://chat.openai.com/*",
                "https://chatgpt.com/*"
            ],
            "js": [
                "chatgpt.js"
            ],
            "run_at": "document_idle"
        }
    ]
}
